{
  "name": "z-uno",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/z-uno/src",
  "prefix": "configurator",
  "tags": ["Z-Uno 2"],
  "projectType": "library",
  "targets": {
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/z-uno/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/z-uno/**/*.ts", "libs/z-uno/**/*.html"]
      }
    }
  }
}
